{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3|^7.3|^8.0",
        "ext-calendar": "*",
        "10quality/php-calendar": "*",
        "ab-alselwi/laravel-arabic-html": "^0.2.0",
        "barryvdh/laravel-dompdf": "^2.1",
        "beyondcode/laravel-websockets": "^2.0@beta",
        "charlieuki/receiptprinter": "^1.2",
        "deeplcom/deepl-php": "^1.7",
        "dompdf/dompdf": "^2.0",
        "fisharebest/ext-calendar": "*",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^3.5",
        "khaled.alshamaa/ar-php": "^6.3.4",
        "kutia-software-company/larafirebase": "^1.3",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.3",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.8",
        "milon/barcode": "^11.0",
        "pragmarx/firewall": "^2.3",
        "propaganistas/laravel-phone": "^5.1",
        "pusher/pusher-php-server": "^7.2",
        "ralphjsmit/laravel-seo": "^1.6",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-translatable": "^6.6",
        "stevebauman/location": "^6.5",
        "symfony/http-foundation": "^6.4",
        "symfony/intl": "^6.4",
        "torann/geoip": "^3.0",
        "victorybiz/laravel-tel-input": "^1.6",
        "yajra/laravel-datatables-buttons": "9.0",
        "yajra/laravel-datatables-oracle": "^10.11"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.4",
        "laravel/sail": "^1.29",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.8",
        "spatie/laravel-ignition": "^1.0",
        "timwassenburg/laravel-service-generator": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}